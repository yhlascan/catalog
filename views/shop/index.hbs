  <!-- veg section -->

  <section class="veg_section layout_padding">
    <div class="container">
      <div class="heading_container heading_center">
        <h2>
          Наши Продукты
        </h2>
        <p>
          Быстро,удобно и свежо!
        </p>
      </div>
      {{# each products }}
      <div class="row">
        {{# each this }}
        <div class="col-md-6 col-lg-4">
          <div class="box">
            <div class="img-box">
              <img src="{{this.imagePath}}" alt="{{this.title}}">
            </div>
            <div class="detail-box">
              <a href="">
                {{this.title}}
              </a>
              <div class="price_box">
                <h6 class="price_heading">
                 {{this.price}}<span>TMT</span> 
                </h6>
              </div>
            </div>
          </div>
        </div>
        {{/each}}
      </div>
      {{/each}}
    </div>
  </section>

  <!-- end veg section -->